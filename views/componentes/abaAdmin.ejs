<script>

    const element66 = document.getElementById("tabs-381787046");
    DDS.Tabs(element66);

    if (window.location.pathname === '/admin') {
    document.querySelector('#aba1').setAttribute("aria-selected", "false");
    document.querySelector('#aba2').setAttribute("aria-selected", "true");
} else {
    document.querySelector('#aba1').setAttribute("aria-selected", "true");
    document.querySelector('#aba2').setAttribute("aria-selected", "false");
}


    document.querySelector('#aba2').addEventListener('click', () => {
    if (window.location.pathname != '/admin'){
    window.location.href = '/admin';}
});
document.querySelector('#aba1').addEventListener('click', () => {
    if (window.location.pathname != '/dashboard'){
    window.location.href = '/dashboard';}
});

</script>

